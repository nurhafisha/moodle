<form
  method="post"
  action="{{ path('app_u_e_delete', { idUE: ue.idUE }) }}"
  onsubmit="return confirm('Are you sure you want to delete this item?');"
>
  <input
    type="hidden"
    name="_token"
    value="{{ csrf_token('delete' ~ ue.idUE) }}"
  />
  <button class="btn">Supprimer</button>
</form>
